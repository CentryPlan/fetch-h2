{
  "name": "fetch-h2",
  "version": "0.0.10",
  "description": "HTTP/2-only Fetch API client for Node.js",
  "author": "Gustaf Räntilä",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/grantila/fetch-h2/issues"
  },
  "homepage": "https://github.com/grantila/fetch-h2#readme",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "directories": {},
  "engines": {
    "node": ">=8.4"
  },
  "scripts": {
    "build": "./node_modules/.bin/rimraf dist && ./node_modules/.bin/tsc -p .",
    "test": "node --expose-http2 node_modules/.bin/_mocha dist/test",
    "buildtest": "npm run build && npm run test",
    "buildtestfast": "npm run build && node --expose-http2 node_modules/.bin/_mocha --grep basic dist/test",
    "version": "./node_modules/.bin/ts-node scripts/version-update.ts && npm run buildtest && scripts/version-git-add.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grantila/fetch-h2.git"
  },
  "keywords": [
    "fetch",
    "h2",
    "http2",
    "client",
    "api",
    "typesafe",
    "typescript"
  ],
  "devDependencies": {
    "@types/from2": "2.x",
    "@types/get-stream": "3.x",
    "@types/mocha": "2.x",
    "@types/node": "8.x",
    "@types/through2": "2.x",
    "@types/tough-cookie": "2.x",
    "chai": "4.x",
    "from2": "2.x",
    "mocha": "4.x",
    "rimraf": "2.x",
    "ts-node": "3.x",
    "typescript": "2.5.x"
  },
  "dependencies": {
    "already": "0.x",
    "callguard": "1.x",
    "get-stream": "3.x",
    "is-buffer": "1.x",
    "through2": "2.x",
    "to-arraybuffer": "1.x",
    "tough-cookie": "2.x"
  }
}
